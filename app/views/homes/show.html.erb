<div class="largeContainer">

    <div class="mapBG">
      <div class="textContainerData">
        <span class="bigParallaxText">Compare kiosks.</span>
      </div>
    </div>
  <div class="contentContainer">

    <div class="bigContent left">
        <h2> Trips started at station by month
        <br>
        from  <%= @pretty_month_start %> to <%= @pretty_month_end %></h2>



        <div class="lineChart">
          <%= line_chart @station_variables,
            colors: ["#86b300", "#b30086", "#2d00b3", "#0086b3", "#e6ad00"],
            library: {backgroundColor: "#eee"} %>
        </div>
        <h2> Trips started at station by day
        <br>
        from  <%= @pretty_month_start %> to <%= @pretty_month_end %></h2>


          <div class="lineChart">
          <%= line_chart @station_variables_daily,
            colors: ["#86b300", "#b30086", "#2d00b3", "#0086b3", "#e6ad00"],
            library: {backgroundColor: "#eee"} %>
        </div>

        <h2> Trips started at station by day of week
        <br>
        from  <%= @pretty_month_start %> to <%= @pretty_month_end %></h2>


          <div class="lineChart">
          <%= column_chart @station_variables_weekday,
            colors: ["#86b300", "#b30086", "#2d00b3", "#0086b3", "#e6ad00"],
            library: {backgroundColor: "#eee"} %>
        </div>

      </div>
      <div class="content rightForm">
        <%= render 'months' %>
        <br>
      </div>
</div>
